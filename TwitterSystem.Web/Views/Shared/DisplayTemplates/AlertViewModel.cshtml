@model TwitterSystem.Web.ViewModels.Alerts.AlertViewModel

<div class="alert alert-@Model.CssClass alert-dismissible text-center col-md-8 col-md-offset-2" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    @Model.Message
</div>
